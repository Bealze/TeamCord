<Window x:Class="TeamCord.GUI.TotpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeamCord.GUI"
        mc:Ignorable="d"
        WindowStyle="ToolWindow"
        ResizeMode="NoResize"
        Title="" Height="150" Width="250">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Label Content="Enter two factor code below" />
            <TextBox Name="textBox_Code" />
            <Button Name="button_Login" Content="Login" Width=" 70" Margin="20" Click="button_Login_Click" />
        </StackPanel>
    </Grid>
</Window>